<table class="list">
  <thead>
  <tr>
    <th>Наименование</th>
    <th>Инвентарный №</th>
    <th>Серийный №</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @equipments.each do |equipment| %>
    <tr class="hascontextmenu <%= cycle('odd', 'even') %>">
      <td><%= equipment.stationname %></td>
      <td><%= equipment.invnumber %></td>
      <td><%= equipment.sernumber %></td>
      <td><%#= link_to 'Show', equipment %></td>
      <td><%#= link_to 'Edit', edit_equipment_path(equipment) %></td>
      <td><%#= link_to 'Destroy', equipment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>